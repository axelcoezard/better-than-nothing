{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/Modules/Core/src/**",
                "${workspaceFolder}/Modules/Runtime/src/**",
				"${workspaceFolder}/ThirdParty/glfw/include/**",
				"${workspaceFolder}/ThirdParty/vulkan/include/**",
				"${workspaceFolder}/ThirdParty/glm/include/**",
				"${workspaceFolder}/ThirdParty/stb/include/**",
				"${workspaceFolder}/ThirdParty/tinyobjloader/**",
				"${workspaceFolder}/ThirdParty/entt/single_include/**",
				"${workspaceFolder}/ThirdParty/imgui/include/**"
            ],
            "compilerPath": "/usr/bin/g++",
            "forcedInclude": [
                "${workspaceFolder}/build/Modules/Core/CMakeFiles/Core.dir/cmake_pch.hxx",
                "${workspaceFolder}/build/Modules/Runtime/CMakeFiles/Runtime.dir/cmake_pch.hxx"
            ],
            "cStandard": "c17",
            "intelliSenseMode": "linux-gcc-x64",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}
